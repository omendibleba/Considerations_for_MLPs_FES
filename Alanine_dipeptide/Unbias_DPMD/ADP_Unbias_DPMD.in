#pair_coeff      * * ../Model_train_SPC/ADP_SPC_Unbias_1000.pth H C N O
#pair_coeff      * * ../Model_train_SPC/ADP_SPC_Unbias_2500.pth H C N O
#pair_coeff      * * ../Model_train_SPC/ADP_SPC_Omin_500.pth H C N O
#pair_coeff      * * ../Model_train_SPC/ADP_SPC_Omin_1000.pth H C N O
#pair_coeff      * * ../Model_train_SPC/ADP_SPC_Omin_2500.pth H C N O
#pair_coeff      * * ../Model_train_SPC/ADP_SPC_CharReg_500.pth H C N O
#pair_coeff      * * ../Model_train_SPC/ADP_SPC_CharReg_1000.pth H C N O
#pair_coeff      * * ../Model_train_SPC/ADP_SPC_CharReg_2500.pth H C N O


############ For models trained with classical level of accuracy (CLC)############
#pair_coeff      * * ../Model_train_CLC/ADP_CLC_UnivDist_2500.pth H C N O
#pair_coeff      * * ../Model_train_CLC/ADP_CLC_UnivDist_5000.pth H C N O
#pair_coeff      * * ../Model_train_CLC/ADP_CLC_BoltzDist_2500.pth H C N O
#pair_coeff      * * ../Model_train_CLC/ADP_CLC_BoltzDist_5000.pth H C N O
#pair_coeff      * * ../Model_train_CLC/ADP_CLC_Unbias_500.pth H C N O
#pair_coeff      * * ../Model_train_CLC/ADP_CLC_Unbias_1000.pth H C N O

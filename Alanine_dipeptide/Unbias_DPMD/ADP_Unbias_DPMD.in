


# Define name of log file 
log adp_dpmd_1ns_umb_0.log
# Model system


units	real
atom_style atomic
newton on ## off for nequip

read_data ADP.lmp

pair_style	allegro ##nequip

############ For models trained with ab initio level of accuracy (SPC)###########
pair_coeff      * * ../Model_train_SPC/ADP_SPC_UnivDist_2500.pth H C N O
#pair_coeff      * * ../Model_train_SPC/ADP_SPC_UnivDist_5000.pth H C N O
#pair_coeff      * * ../Model_train_SPC/ADP_SPC_BoltzDist_2500.pth H C N O
